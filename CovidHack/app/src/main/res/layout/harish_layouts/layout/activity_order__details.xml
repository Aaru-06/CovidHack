<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".activities.clement_activities.Order_Details">

    <TextView
        android:id="@+id/dop"
        android:layout_width="214dp"
        android:layout_height="40dp"
        android:layout_gravity="center"
        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/custname"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.862"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.748" />

    <TextView
        android:id="@+id/orderid"
        android:layout_width="109dp"
        android:layout_height="36dp"
        android:text="Order_ID"
        app:layout_constraintBottom_toTopOf="@+id/textView18"
        app:layout_constraintEnd_toStartOf="@+id/dop"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView17" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="198dp"
        android:layout_height="41dp"
        android:text="Order-Details"
        app:layout_constraintBottom_toTopOf="@+id/dop"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="110dp"
        android:layout_height="32dp"
        android:layout_marginStart="28dp"
        android:layout_marginBottom="64dp"
        android:text="Customer Name"
        app:layout_constraintBottom_toTopOf="@+id/viewcart"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/custname"
        android:layout_width="212dp"
        android:layout_height="35dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="212dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/textView18"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/viewcart"
        android:layout_width="149dp"
        android:layout_height="46dp"
        android:layout_marginTop="80dp"
        android:layout_marginEnd="144dp"
        android:text="View Cart"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/custname" />
</androidx.constraintlayout.widget.ConstraintLayout>